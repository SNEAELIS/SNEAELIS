  <script>
    // Função para calcular o % Executado
    function calculatePercentage(previstoId, executadoId, percentualId) {
      const previsto = parseFloat(document.getElementById(previstoId).value) || 0;
      const executado = parseFloat(document.getElementById(executadoId).value) || 0;

      const percentual = previsto > 0 ? ((executado / previsto) * 100).toFixed(2) : '0.00';

      document.getElementById(percentualId).value = `${percentual}%`;
    }

     // Função para calcular o % Executado
  function calculatePercentage(previstoId, executadoId, percentualId) {
    const previsto = parseFloat(document.getElementById(previstoId).value) || 0;
    const executado = parseFloat(document.getElementById(executadoId).value) || 0;

    const percentual = previsto > 0 ? ((executado / previsto) * 100).toFixed(2) : '0.00';

    document.getElementById(percentualId).value = `${percentual}%`;

    calculateTotal();
  }

  // Função para calcular totais gerais
  function calculateTotal() {
    const idsPrevisto = ['previsto_pessoal', 'previsto_tributos', 'previsto_grafico'];
    const idsExecutado = ['executado_pessoal', 'executado_tributos', 'executado_grafico'];

    let totalPrevisto = 0;
    let totalExecutado = 0;

    idsPrevisto.forEach(id => {
      totalPrevisto += parseFloat(document.getElementById(id).value) || 0;
    });

    idsExecutado.forEach(id => {
      totalExecutado += parseFloat(document.getElementById(id).value) || 0;
    });

    const totalPercentual = totalPrevisto > 0 ? ((totalExecutado / totalPrevisto) * 100).toFixed(2) : '0.00';

    document.getElementById('total_previsto').value = totalPrevisto.toFixed(2);
    document.getElementById('total_executado').value = totalExecutado.toFixed(2);
    document.getElementById('total_percentual').value = `${totalPercentual}%`;
  }

  function calculateTotal() {
    // IDs dos campos de valor
    const valoresIds = ['valor_1', 'valor_2'];

    // Calcula o total
    let total = 0;
    valoresIds.forEach(id => {
      total += parseFloat(document.getElementById(id).value) || 0;
    });

    // Exibe o total no campo correspondente
    document.getElementById('total_valor').value = total.toFixed(2);
  }

  function calculateMeta1() {
    // IDs de todas as linhas
    const rows = [
      { previsto: 'previsto_pessoal', executado: 'executado_pessoal', percentual: 'percentual_pessoal' },
      { previsto: 'previsto_tributos', executado: 'executado_tributos', percentual: 'percentual_tributos' },
      { previsto: 'previsto_uniformes', executado: 'executado_uniformes', percentual: 'percentual_uniformes' },
      { previsto: 'previsto_material', executado: 'executado_material', percentual: 'percentual_material' },
      { previsto: 'previsto_premiacao', executado: 'executado_premiacao', percentual: 'percentual_premiacao' },
      { previsto: 'previsto_locacao', executado: 'executado_locacao', percentual: 'percentual_locacao' },
    ];

    let totalPrevisto = 0;
    let totalExecutado = 0;

    rows.forEach(row => {
      const previsto = parseFloat(document.getElementById(row.previsto).value) || 0;
      const executado = parseFloat(document.getElementById(row.executado).value) || 0;

      const percentual = previsto > 0 ? ((executado / previsto) * 100).toFixed(2) : '0.00';

      // Atualiza o percentual da linha
      document.getElementById(row.percentual).value = `${percentual}%`;

      // Soma os valores totais
      totalPrevisto += previsto;
      totalExecutado += executado;
    });

    // Calcula o percentual total
    const totalPercentual = totalPrevisto > 0 ? ((totalExecutado / totalPrevisto) * 100).toFixed(2) : '0.00';

    // Atualiza os totais
    document.getElementById('total_previsto_meta1').value = totalPrevisto.toFixed(2);
    document.getElementById('total_executado_meta1').value = totalExecutado.toFixed(2);
    document.getElementById('total_percentual_meta1').value = `${totalPercentual}%`;
  }

      // Alternância entre abas
    document.querySelectorAll('.tab-link').forEach(button => {
      button.addEventListener('click', () => {
        const target = button.getAttribute('data-target');

        // Remove 'active' de todas as abas
        document.querySelectorAll('.tab-link').forEach(btn => btn.classList.remove('active'));
        document.querySelectorAll('section').forEach(section => section.classList.remove('active'));

        // Adiciona 'active' à aba clicada
        button.classList.add('active');
        document.getElementById(target).classList.add('active');
      });
    });  

